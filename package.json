{
  "name": "tweaked",
  "description": "Fine tune your data.",
  "author": "Automators",
  "type": "module",
  "version": "0.0.4",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:frontend": "prettier --ignore-path ./.prettierignore --write \"**/*.+(js|jsx|tsx|ts|astro|json)\"",
    "format:rust": "cd src-tauri && cargo fmt && cd ..",
    "format": "pnpm run format:frontend && pnpm run format:rust",
    "astro": "astro",
    "tauri": "tauri",
    "server:venv": "python3 -m venv .venv",
    "server:activate": "source .venv/bin/activate",
    "server:dev": "cd server && ../.venv/bin/uvicorn main:app --reload",
    "server:start": "pnpm run server:activate && cd server && ../.venv/bin/uvicorn main:app",
    "server:install": "pnpm run server:venv && pnpm run server:activate && .venv/bin/pip install -r ./server/requirements.txt"
  },
  "dependencies": {
    "@nanostores/persistent": "^0.10.1",
    "@nanostores/react": "^0.7.2",
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/react-query": "^5.40.1",
    "@tauri-apps/api": "^1.5.6",
    "@tauri-apps/cli": "2.0.0-beta.20",
    "@tauri-apps/plugin-dialog": "2.0.0-beta.5",
    "@tauri-apps/plugin-process": "2.0.0-beta.5",
    "@tauri-apps/plugin-updater": "2.0.0-beta.5",
    "@thumbmarkjs/thumbmarkjs": "^0.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "daisyui": "^4.11.1",
    "nanostores": "^0.10.3",
    "next": "15.0.0-rc.0",
    "next-themes": "^0.3.0",
    "react": "19.0.0-rc-f994737d14-20240522",
    "react-dom": "19.0.0-rc-f994737d14-20240522",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.5.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.0-rc.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-rust": "^0.1.9",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
